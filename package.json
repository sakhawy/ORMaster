{
  "name": "ormaster",
  "displayName": "ormaster",
  "description": "An extension that helps you mastering the ORM of your favorite web framework.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ormaster.setupChallenge",
        "title": "Setup Challenge"
      },
      {
        "command": "ormaster.submitChallenge",
        "title": "Submit Challenge"
      }
    ],
    "views": {
      "ormaster-explorer": [
        {
          "id": "ormaster",
          "name": "ORMaster",
          "icon": "media/ormaster-icon.svg",
          "contextualTitle": "ORMaster"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ormaster-explorer",
          "title": "ORMaster Explorer",
          "icon": "media/ormaster-icon.svg"
        }
      ]
    }
  },
  "capabilities": {
    "codeLensProvider": {
      "resolveProvider": true
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "fs-extra": "^11.1.1"
  }
}
